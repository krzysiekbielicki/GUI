package pl.edu.pjwstk.s8267.GUI.Lab5;

import java.util.Arrays;
import java.util.Comparator;

public class PozycjaBilingu implements Comparator<PozycjaBilingu> {
	private static final String KEY_TIME = null;
	private static final String KEY_LENGTH = null;
	private static final String KEY_NAME = null;

	public PozycjaBilingu(String string, int i, int j, int k) {
		// TODO Auto-generated constructor stub
	}

	private static void setSortKey(String keyLength) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public int compare(PozycjaBilingu o1, PozycjaBilingu o2) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	public static void main(String[] args) {
		PozycjaBilingu biling[] = { new PozycjaBilingu("Arek", 5, 59, 94),
				new PozycjaBilingu("Zosia", 17, 5, 61),
				new PozycjaBilingu("Mat", 12, 0, 180) };
		for (PozycjaBilingu b : biling)
			System.out.println(b);

		PozycjaBilingu.setSortKey(PozycjaBilingu.KEY_TIME);
		Arrays.sort(biling);
		for (PozycjaBilingu b : biling)
			System.out.println(b);

		PozycjaBilingu.setSortKey(PozycjaBilingu.KEY_LENGTH);
		Arrays.sort(biling);
		for (PozycjaBilingu b : biling)
			System.out.println(b);

		PozycjaBilingu.setSortKey(PozycjaBilingu.KEY_NAME);
		Arrays.sort(biling);
		for (PozycjaBilingu b : biling)
			System.out.println(b);
	}
}
